def hallo(param, arg, p) begin:
    write(param, arg, p);
    succ(param);
    return param;
end

def add(s1, s2) begin:
    var retVal := s1;

    while(s2) begin:
        succ(retVal);
        pred(s2);
    end

    return retVal;
end

def mul(s1, s2) begin:
    var retVal := s1;
    pred(s2);

    while(s2) begin:
        retVal := add(retVal, s1);
        pred(s2);
    end

    return retVal;
end

def isZero(s1) begin:
    var retVal := 1;

    while(s1) begin:
        retVal := 0;
        s1 := 0;
    end

    return retVal;
end
 
